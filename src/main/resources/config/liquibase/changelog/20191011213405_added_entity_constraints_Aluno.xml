<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Aluno.
    -->
    <changeSet id="20191011213405-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="agenda_atendimento_servidor_id"
                                 baseTableName="aluno"
                                 constraintName="fk_aluno_agenda_atendimento_servidor_id"
